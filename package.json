{
  "name": "angular2-webpack2-labs",
  "version": "0.0.1",
  "author": "apozgaj",
  "scripts": {
    "ngc": "./node_modules/.bin/ngc -p tsconfig.aot.json",
    "ngc:localized": "node scripts/ngc-localized.js",
    "webpack": "webpack",
    "postcss": "postcss",
    "tslint": "tslint",
    "lint": "npm run tslint \"src/app/**/*.ts\" ",
    "sass": "node-sass src -o src --output-style compressed -q",
    "postsass": "npm run postcss -- -c postcssconfig.json",
    "sass:watch": "node-sass -w src -o src --output-style compressed -q",
    "rimraf": "rimraf",
    "build:dll": "npm run clean:dll && npm run sass && webpack",
    "compile:dev": "npm run lint && npm run sass && npm run build:dev",
    "compile:prod": "npm run lint && npm run sass && npm run clean:compile && npm run ngc:localized && npm run build:prod",
    "build:dev": "npm run clean:dist && npm run sass && webpack",
    "build:prod": "npm run clean:dist && npm run sass && webpack",
    "build:translation": "./node_modules/.bin/ng-xi18n  -p tsconfig.aot.json --i18nFormat=xlf",
    "clean:dll": "npm run rimraf -- build/dll",
    "clean:dist": "npm run rimraf -- build/dist .awcache",
    "clean:compile": "npm run rimraf -- \"build/aot\" ",
    "watch:dev": "npm-run-all -p -r \"build:dev -- --watch\" sass:watch",
    "test:dev": "karma start",
    "test:prod": "karma start"
  },
  "dependencies": {
    "@angular/common": "2.2.3",
    "@angular/compiler": "2.2.3",
    "@angular/compiler-cli": "2.2.3",
    "@angular/core": "2.2.3",
    "@angular/forms": "2.2.3",
    "@angular/http": "2.2.3",
    "@angular/platform-browser": "2.2.3",
    "@angular/platform-browser-dynamic": "2.2.3",
    "@angular/platform-server": "2.2.3",
    "@angular/router": "3.2.3",
    "@angular/tsc-wrapped": "0.3.0",
    "angular2localization": "1.2.0",
    "core-js": "2.4.1",
    "rxjs": "5.0.0-rc.4",
    "zone.js": "0.7.1",
    "ts-helpers": "1.1.2",
    "ie-shim": "0.1.0",
    "reflect-metadata": "^0.1.8"
  },
  "devDependencies": {
    "@types/jasmine": "^2.2.29",
    "@types/js-base64": "^2.1.4",
    "@types/node": "^4.0.29",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "angular2-template-loader": "0.6.0",
    "aspnet-webpack": "^1.0.9",
    "awesome-typescript-loader": "2.2.4",
    "babel-core": "^6.18.2",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "codelyzer": "~0.0.21",
    "compression-webpack-plugin": "0.3.2",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "^2.0.1",
    "css-loader": "0.25.0",
    "es6-shim": "0.35.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.21.0",
    "intl": "1.2.5",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-intl-shim": "^1.0.3",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.1.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-nyan-reporter": "0.2.4",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "node-sass": "^3.12.2",
    "npm-run-all": "^3.1.1",
    "postcss": "^5.2.6",
    "postcss-cli": "2.6.0",
    "postcss-flexbugs-fixes": "2.0.0",
    "raw-loader": "0.5.1",
    "reflect-metadata": "^0.1.3",
    "source-map-loader": "0.1.5",
    "style-loader": "^0.13.1",
    "ts-node": "^1.2.3",
    "tslint": "^3.7.1",
    "tslint-loader": "^2.1.3",
    "typescript": "2.0.9",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.26",
    "webpack-bundle-analyzer": "1.5.3",
    "webpack-dashboard": "0.0.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
